<Window x:Class="U5_Challange_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U5_Challange_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Label>Loonies:</Label>
        <TextBox x:Name="Loonies"></TextBox>
        <Label>Twooonies:</Label>
        <TextBox x:Name="Twoonies"></TextBox>
        <Label>Nickles:</Label>
        <TextBox x:Name="Nickles"></TextBox>
        <Label>Dimes:</Label>
        <TextBox x:Name="Dimes"></TextBox>
        <Label>Quarters:</Label>
        <TextBox x:Name="Quarters"></TextBox>
        <Button x:Name="AddingButton" Click="AddingButton_Click">Sum</Button>
        <Label x:Name="Equal"></Label>
    </StackPanel>
</Window>
